		<!-- 수익분석 패키지 -->
		<typeAlias alias="fundVo" type="kr.or.ddit.marginAnalysis.model.FundVo"/>
		<typeAlias alias="expenseVo" type="kr.or.ddit.marginAnalysis.model.ExpenseVo"/>
		<typeAlias alias="avgSalesVo" type="kr.or.ddit.marginAnalysis.model.AvgSalesVo"/>
		<typeAlias alias="marginVo" type="kr.or.ddit.marginAnalysis.model.MarginVo"/>
