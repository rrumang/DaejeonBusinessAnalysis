<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	<typeAliases>
		<!-- select 패키지 -->
		<typeAlias alias="regionVo" type="kr.or.ddit.select.model.RegionVo"/>
		<typeAlias alias="tobVo" type="kr.or.ddit.select.model.TobVo"/>
		
		<!-- data 패키지 -->
		<typeAlias alias="lpVo" type="kr.or.ddit.data.model.LpVo"/>
		<typeAlias alias="wpVo" type="kr.or.ddit.data.model.WpVo"/>
		<typeAlias alias="ppaVo" type="kr.or.ddit.data.model.PpaVo"/>
		<typeAlias alias="ppgVo" type="kr.or.ddit.data.model.PpgVo"/>
		<typeAlias alias="salesVo" type="kr.or.ddit.data.model.SalesVo"/>
		<typeAlias alias="latlngVo" type="kr.or.ddit.data.model.LatLngVo"/>
