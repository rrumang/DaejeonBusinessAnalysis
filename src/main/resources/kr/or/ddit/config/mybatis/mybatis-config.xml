		<!-- 분석보고서 패키지 -->
		<typeAlias alias="reportVo" type="kr.or.ddit.report.model.ReportVo"/>
		
		<!-- member 패키지 -->
		<typeAlias alias="memberVo" type="kr.or.ddit.member.model.MemberVo"/>
		
