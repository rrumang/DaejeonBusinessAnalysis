		<!-- 페이징 처리 패키지 -->
		<typeAlias alias="pageVo" type="kr.or.ddit.paging.model.PageVo"/>
		
		<!-- 공지사항 패키지 -->
		<typeAlias alias="noticeVo" type="kr.or.ddit.notice.model.NoticeVo"/>
		
		<!-- 건의사항 패키지 -->
		<typeAlias alias="suggestionVo" type="kr.or.ddit.suggestion.model.SuggestionVo"/>
		
		<!-- 첨부파일 패키지 -->
		<typeAlias alias="attachVo" type="kr.or.ddit.attach.model.AttachVo"/>
		
	</typeAliases>
	
	<mappers>
		<mapper resource="kr/or/ddit/config/mybatis/mapper/tobRecom.xml"/>
		<mapper resource="kr/or/ddit/config/mybatis/mapper/faq.xml"/>
		<mapper resource="kr/or/ddit/config/mybatis/mapper/report.xml"/>
